    "noImplicitAny": false,
    "noEmitOnError": false,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "outFile": "built/out.js"
  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts"
  ],
